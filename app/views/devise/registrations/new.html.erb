<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

 <div> <%= f.text_field :first_name ,:placeholder=>"First Name"%></div>
   <div class="field">
   
    <%= f.text_field :lasst_name, :placeholder=>"Last Name" %>
  </div>
  <div>
  <%= f.email_field :email, :autofocus => true ,:placeholder=>"email"%></div>

  <div>
  <%= f.password_field :password , :placeholder=>"password"%></div>

  <div>
  <%= f.password_field :password_confirmation, :placeholder=>"confirm" %></div>

  <div><%= f.submit "Sign up", :class=>"btn btn-primary" %></div>
<% end %>

<%= render "devise/shared/links" %>
